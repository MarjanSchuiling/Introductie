
const number = 9;

for(let i = 1; i <= 10; i++) {
    // multiply i with number
    const result = i * number;
    // display the result
    console.log(`${number} * ${i} = ${result}`);
}

const number1 = 1;

for(let i = 1; i <= 10; i++) {
    const result = i * number1;
    console.log(`${number1} * ${i} = ${result}`);
}

const number2 = 2;

for(let i = 1; i <= 10; i++) {
    const result = i * number2;
    console.log(`${number2} * ${i} = ${result}`);
}

